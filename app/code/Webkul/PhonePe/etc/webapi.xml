<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/PhonePe/" method="POST">
        <service class="Webkul\PhonePe\Api\WebhookInterface" method="executeWebhook"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/CreditMemo/" method="POST">
        <service class="Webkul\PhonePe\Api\CreditMemoInterface" method="execute"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
